{
	"name": "scandltd/rhenus-wms",
	"description": "Rhenus WMS Library",
	"version": "1.0.0",
	"time": "2018-07-24",
	"keywords": ["PHP", "Rhenus", "logistics", "WMS", "lib", "ftp"],
	"type": "library",
	"license": "GPLv2",
	"authors": [
		{
			"name": "Alex Burets",
			"email": "aller@scand.com"
		},
		{
			"name": "Andrew Simonov",
			"email": "simanau@scand.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"php-curl-class/php-curl-class": "^4.11"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.3",
        "squizlabs/php_codesniffer": "2.*"
	},
	"autoload": {
		"psr-4": {
			"Scand\\RhenusWMS\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Scand\\RhenusWMS\\Tests\\": "tests/"
		}
	},
	"config": {
		"bin-dir": "vendor/bin",
		"platform": {
			"php": "5.6"
		}
	},
	"scripts": {
		"test": [
			"phpunit"
		]
	}
}
